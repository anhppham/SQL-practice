| id | num | id | num |
| -- | --- | -- | --- |
| 0  | -9  | 1  | -9  |
| 1  | -9  | 2  | -9  |
| 12 | -8  | 13 | -8  |
| 34 | -7  | 35 | -7  |
| 47 | -8  | 48 | -8  |
| 72 | 2   | 73 | 2   |
| 73 | 2   | 74 | 2   |
| 75 | -4  | 76 | -4  |

select * #l1.num ConsecutiveNums
from logs l1 inner join logs l2 on l1.id+1 = l2.id
where l1.num = l2.num
group by ConsecutiveNums
having count(*) >= 2


#correction solution
| id | num | id | num | id | num |
| -- | --- | -- | --- | -- | --- |
| 0  | -9  | 1  | -9  | 2  | -9  |
| 72 | 2   | 73 | 2   | 74 | 2   |

select distinct l1.num ConsecutiveNums
from (logs l1 join logs l2 on l1.id+1 = l2.id and l1.num = l2.num) 
join logs l3 on l2.id+1 = l3.id and l1.num = l3.num
